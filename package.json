{
  "name": "api-prisma",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "private": "true",
  "scripts": {
    "build": "tsc",
    "dev": "yarn docker:down && yarn docker:up && yarn start",
    "start": "tsnd --respawn -r dotenv/config --transpile-only src/server.ts",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:destroy": "docker-compose down --volumes --remove-orphans",
    "test": "yarn test:unit",
    "test:unit": "jest",
    "test:watch": "jest --watch",
    "migrate": "prisma migrate dev --name init",
    "migration": "prisma migrate dev",
    "migration:prod": "prisma migrate deploy"
  },
  "description": "",
  "author": "",
  "engines": {
    "node": "18.x"
  },
  "keywords": [
    "node",
    "typescript",
    "api"
  ],
  "dependencies": {
    "@prisma/client": "4.4.0",
    "bcrypt": "5.1.0",
    "cors": "2.8.5",
    "dotenv": "16.0.3",
    "express": "4.18.1",
    "jsonwebtoken": "9.0.0",
    "uuid": "9.0.0"
  },
  "devDependencies": {
    "@types/bcrypt": "5.0.0",
    "@types/cors": "^2.8.13",
    "@types/express": "4.17.14",
    "@types/jest": "29.4.0",
    "@types/jsonwebtoken": "9.0.2",
    "@types/node": "18.14.0",
    "@types/uuid": "9.0.0",
    "@typescript-eslint/eslint-plugin": "5.56.0",
    "@typescript-eslint/parser": "5.56.0",
    "eslint": "8.36.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "27.1.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-promise": "6.0.1",
    "jest": "29.4.3",
    "prettier": "2.8.8",
    "prisma": "4.10.1",
    "ts-jest": "29.0.5",
    "ts-node-dev": "2.0.0",
    "typescript": "4.8.4"
  }
}
