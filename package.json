{
  "name": "api-prisma",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "private": "true",
  "scripts": {
    "build": "tsc",
    "dev": "tsnd --transpile-only --ignore-watch node_modules src/server.ts",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:destroy": "docker-compose down --volumes --remove-orphans",
    "migrate": "prisma migrate dev --name",
    "migration": "prisma migrate dev",
    "migration:prod": "prisma migrate deploy"
  },
  "dependencies": {
    "@prisma/client": "4.4.0",
    "dotenv": "16.0.3",
    "express": "4.18.1",
    "uuid": "9.0.0"
  },
  "devDependencies": {
    "@types/express": "4.17.14",
    "@types/uuid": "9.0.0",
    "@typescript-eslint/eslint-plugin": "5.39.0",
    "@typescript-eslint/parser": "5.39.0",
    "eslint": "8.24.0",
    "eslint-config-standard": "17.0.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "27.1.1",
    "eslint-plugin-n": "15.3.0",
    "eslint-plugin-promise": "6.0.1",
    "jest": "29.1.2",
    "prisma": "4.10.1",
    "ts-node-dev": "2.0.0",
    "typescript": "4.8.4"
  }
}
